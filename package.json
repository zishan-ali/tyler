{
  "name": "tyler",
  "version": "2.0.7",
  "description": "An interactive React.js CLI for building atomic components & utilities.",
  "preferGlobal": true,
  "authors": [
    {
      "name": "Zishan Ali"
    },
    {
      "name": "Robert White"
    }
  ],
  "license": "ISC",
  "keywords": [
    "generator",
    "creator",
    "cli",
    "tool"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zishan-ali/tyler"
  },
  "bugs": {
    "url": "https://github.com/zishan-ali/tyler/issues"
  },
  "homepage": "https://zishan-ali.github.io/tyler/",
  "files": [
    "dist"
  ],
  "main": "dist/src/index.js",
  "bin": {
    "create": "dist/bin/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:bin": "babel bin -d dist/bin",
    "build:src": "babel src -d dist/src",
    "build": "yarn clean && yarn build:bin && yarn build:src",
    "lint": "eslint ./src/**/*.js",
    "lint:fix": "eslint **/*.js --fix",
    "test": "jest test"
  },
  "linters": {
    "src/**/*.{js}": [
      "eslint"
    ]
  },
  "dependencies": {
    "inquirer": "^5.2.0",
    "meow": "^4.0.0",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-airbnb": "^2.5.1",
    "eslint": "^5.6.1",
    "jest": "^23.5.0",
    "mock-fs": "^4.4.2"
  }
}
